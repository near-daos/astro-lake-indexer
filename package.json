{
   "name": "near-lake-astro-poc",
   "version": "1.0.0",
   "description": "",
   "main": "build/index.js",
   "scripts": {
      "build": "tsc --build",
      "clean": "tsc --build --clean",
      "start": "ts-node src/index.ts",
      "start:debug": "LOG_LEVEL=debug npm run start",
      "start:trace": "LOG_LEVEL=trace npm run start",
      "format": "prettier --write \"src/**/*.ts\"",
      "lint": "eslint --ext .ts src/",
      "lint:fix": "eslint --ext .ts src/ --fix",
      "pretest": "npm run lint",
      "test": "echo \"Error: no test specified\" && exit 1",
      "typeorm": "typeorm-ts-node-commonjs -d src/data-source.ts",
      "migration:show": "npm run typeorm migration:show",
      "migration:run": "npm run typeorm migration:run",
      "migration:generate": "npm run typeorm migration:generate -n SchemaRefactoring",
      "migration:revert": "npm run typeorm migration:revert",
      "schema:log": "npm run typeorm schema:log",
      "schema:sync": "npm run typeorm schema:sync",
      "schema:drop": "npm run typeorm schema:drop"
   },
   "author": "",
   "license": "ISC",
   "engines": {
      "npm": ">=7.0.0",
      "node": ">=16.0.0"
   },
   "devDependencies": {
      "@types/log4js": "2.3.5",
      "@types/nconf": "0.10.2",
      "@types/node": "16.11.36",
      "@typescript-eslint/eslint-plugin": "5.25.0",
      "@typescript-eslint/parser": "5.25.0",
      "eslint": "8.15.0",
      "eslint-config-prettier": "8.5.0",
      "eslint-plugin-prettier": "4.0.0",
      "prettier": "2.6.2",
      "ts-node": "10.7.0",
      "typescript": "4.6.4"
   },
   "dependencies": {
      "@supercharge/promise-pool": "2.1.0",
      "aws-sdk": "2.1137.0",
      "dotenv": "16.0.1",
      "log4js": "6.4.7",
      "lru-cache": "7.10.1",
      "nconf": "0.12.0",
      "pg": "8.4.0",
      "reflect-metadata": "0.1.13",
      "typeorm": "0.3.6",
      "typeorm-naming-strategies": "4.1.0"
   }
}
